#php_value upload_max_filesize 40M
#php_value post_max_size 42M
#Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^.kzone.vn
ErrorDocument 404 /404.php
RewriteRule ^(.*)$ http://radio.kzone.vn/$1 [R=permanent,L]
RewriteRule ^admin controller/admin/index.php
RewriteRule ^login controller/admin/login.php

RewriteRule ^tour/([a-zA-Z0-9_-]+)/$ controller/default/chitietdanhmuc.php?Id=$1
RewriteRule ^tour/([a-zA-Z0-9_-]+)/page-([0-9]+)$ controller/default/chitietdanhmuc.php?Id=1&page=$2

RewriteRule ^excursion-tours/$ controller/default/danhmuctour.php
RewriteRule ^excursion-tours/page-([0-9]+)$ controller/default/danhmuctour.php?page=$1

RewriteRule ^vacation-packages/$ controller/default/danhmuctour.php
RewriteRule ^vacation-packages/page-([0-9]+)$ controller/default/danhmuctour.php?page=$1

RewriteRule ^cruise-tours/$ controller/default/danhmuctour.php
RewriteRule ^cruise-tours/page-([0-9]+)$ controller/default/danhmuctour.php?page=$1

RewriteRule ^multi-country/$ controller/default/danhmuctour.php
RewriteRule ^multi-country/page-([0-9]+)$ controller/default/danhmuctour.php?page=$1

RewriteRule ^excursion-tours/$ controller/default/danhmuctour.php
RewriteRule ^excursion-tours/page-([0-9]+)$ controller/default/danhmuctour.php?page=$1

RewriteRule ^destinations/$ controller/default/danhmuctour.php
RewriteRule ^destinations/page-([0-9]+)$ controller/default/danhmuctour.php?page=$1

RewriteRule ^(.*).html$ controller/default/chitiettour.php?Id=$1